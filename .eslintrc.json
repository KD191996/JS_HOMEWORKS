{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-console": 0,
    "func-names": "off",
    "wrap-iife": ["error", "any"],
    "no-extra-parens": ["error", "functions"],
    "no-unused-expressions": "error",
    "quotes": ["error", "single", { "avoidEscape": true }]
  }
}